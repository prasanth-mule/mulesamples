<?xml version='1.0' encoding='UTF-8'?>
<types:mule xmlns:types="http://www.mulesoft.org/schema/mule/types">
  <types:catalog>
    <types:type name="products-data" format="csv">
      <types:shape format="weave" example="sample-input/sample_productcsv_file - Copy.txt"><![CDATA[%dw 2.0

type products_data = Array<{|  productId: String,   name: String,   price: String,   vendor: String|}> {"isQuotedString": "false",
"header": "true",
"headerLineNumber": "0",
"separator": ","}

]]></types:shape>
      <types:properties>
        <types:property name="headerLineNumber" value="0"/>
        <types:property name="header" value="true"/>
        <types:property name="isQuotedString" value="false"/>
        <types:property name="separator" value=","/>
      </types:properties>
    </types:type>
  </types:catalog>
  <types:enrichment select="#808d7b1f-25f8-42df-bb7a-be3fdc9c41ce">
    <types:processor-declaration>
      <types:input-event>
        <types:message>
          <types:payload type="products-data"/>
        </types:message>
      </types:input-event>
    </types:processor-declaration>
  </types:enrichment>
</types:mule>