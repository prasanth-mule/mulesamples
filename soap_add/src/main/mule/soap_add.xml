			<wsc:web-service-security actor="http://schemas.xmlsoap.org/soap/actor/next" />
		</wsc:connection>
	</wsc:config>
	<flow name="soap_addFlow" doc:id="1268906d-511a-46ac-8ae7-4529f749dd74" >
		<http:listener doc:name="Listener" doc:id="1b6f2197-4b78-40b0-8a2b-aff6fb88a9ae" config-ref="HTTP_Listener_config" path="add"/>
		<set-variable value="#[attributes.queryParams.number1]" doc:name="Set Variable" doc:id="12e805d9-bded-4988-a3c8-33ac955d80ee" variableName="p1"/>
