		<ee:transform doc:name="Transform Message" doc:id="0d4cef9a-31e0-4dde-9021-442ae8480d8c" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/xml
ns ns0 http://tempuri.org/
---
{
	ns0#Add: {
		ns0#a: vars.p1 as Number,
		ns0#b: vars.p2 as Number
	}
}]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<wsc:consume operation="Add" doc:name="Consume" doc:id="85a115df-57d9-45fa-97d6-1b30033e3b53" config-ref="Web_Service_Consumer_Config"/>
